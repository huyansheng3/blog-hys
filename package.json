  "scripts": {
    "start": "node scripts/now.js && cross-env PORT=10086 node scripts/start.js",
    "build": "node scripts/now.js && node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "build-deploy": "npm run build && node scripts/rm-map.js && gh-pages --repo https://github.com/huyansheng3/huyansheng3.github.io.git --branch master -d build -m \"deploy $(date)\"",
    "deploy": "node scripts/rm-map.js && gh-pages --repo https://github.com/huyansheng3/huyansheng3.github.io.git --branch master -d build -m \"deploy $(date)\"",
    "generate": "./node_modules/.bin/all-contributors generate",
    "flow": "flow",
    "format": "npm run format:js && npm run format:css && npm run format:json",
    "format:js": "prettier --single-quote --trailing-comma es5 --write '{src,app,__{tests,mocks}__}/**/*.js'",
    "format:css": "prettier --single-quote --write '{src,app}/**/*.css'",
    "format:json": "prettier --single-quote --write '{src,app}/**/*.json'"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "flowtype"
    ],
    "globals": {
      "Phaser": false
    }
  }
}
